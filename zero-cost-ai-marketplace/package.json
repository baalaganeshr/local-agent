{
    "name": "zero-cost-ai-marketplace",
    "private": true,
    "version": "0.1.0",
    "description": "Enterprise-ready MVP of a zero-cost AI marketplace with FastAPI backend and minimal chat UI.",
    "scripts": {
        "serve:frontend": "http-server ./frontend -p 8080 -c-1 --cors",
        "dev:backend": "uvicorn backend.api.main:app --reload --port 8001",
        "dev": "concurrently -k \"npm:dev:backend\" \"npm:serve:frontend\""
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "http-server": "^14.1.1"
    }
}